        -
          name : BTN_FACTURER_COMMANDE
          identifier : 0x25f959b5189ccd16
          internal_properties : 
          properties :
           x : 449
           width : 48
           height : 48
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              code : |1-
               
              type : 14
            -
              code : |1+
               // Vérifie que la commande n'est pas déjà facturée
               SI ZR_Commande[ZR_Commande].INT_EstFacturé ALORS
               	RETOUR
               FIN
               
               // Création de la facture équivalente
               HRAZ(Facture)
               Facture.DateFacture = DateDuJour()
               Facture.NumClient = REQ_COMMANDES.NumClient
               Facture.NumCommande = REQ_COMMANDES.NumCommande
               Facture.TotalHT = REQ_COMMANDES.TotalHT
               Facture.TotalTVA = REQ_COMMANDES.TotalTVA
               Facture.TotalTTC = REQ_COMMANDES.TotalTTC
               HAjoute(Facture)
               
               // Création des lignes de facture équivalentes
               POUR TOUT LigneCde AVEC NumCommande = REQ_COMMANDES.NumCommande
               	HLitRecherchePremier(Produit, Référence, LigneCde.Reference)
               	
               	HRAZ(LigneFac)
               	LigneFac.IDLigneCde = LigneCde.IDLigneCde
               	LigneFac.LibProd = Produit.LibProd
               	LigneFac.NumFacture = Facture.NumFacture
               	LigneFac.PrixVente = LigneCde.PrixVente
               	LigneFac.Quantite = LigneCde.Quantite
               	LigneFac.Reference = LigneCde.Reference
               	HAjoute(LigneFac)
               FIN
               
               // La commande est maintenant facturée
               nLigne est un entier = ZR_Commande
               nPos   est un entier = ZoneRépétéePosition(ZR_Commande)
               ZoneRépétéeAffiche(ZR_Commande, taRéExecuteRequete)
               ZoneRépétéePosition(zr_commande, nPos)
               ZoneRépétéeSelectPlus(ZR_Commande, nLigne)
               
               // Ajout d'un historique
               AjouteHistorique(REQ_COMMANDES.NumClient, cstTYPEHISTO_FACTURE, ChaîneConstruit("Facturation n° %1", Facture.NumFacture))
               
               ToastAffiche("La commande a bien été facturée.")

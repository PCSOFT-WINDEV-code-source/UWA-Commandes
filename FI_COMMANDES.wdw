        -
          name : BTN_SUPPRIMER_COMMANDE
          identifier : 0x25f959b5189acd16
          internal_properties : 
          properties :
           x : 401
           width : 48
           height : 48
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              code : |1-
               
              type : 14
            -
              code : |1+
               // Une ligne est-elle sélectionnée ?
               SI ZoneRépétéeSelect(ZR_Commande)=-1 ALORS RETOUR
               
               HLitRecherchePremier(Commande,NumCommande,ZR_Commande.LIB_NumCde)
               si hendehors(Commande) alors retour
               
               HSupprime(Commande)
               ZoneRépétéeAffiche(ZR_Commande, taRéExecuteRequete)
               ToastAffiche("La commande a été supprimée.")
               
              type : 18
            -

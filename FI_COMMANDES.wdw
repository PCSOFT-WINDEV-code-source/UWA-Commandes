            PROCEDURE MenuBackVisible(bVisible est un booléen = vrai)
            
            	si bVisible ALORS
            		
            		BTN_BACK..visible = vrai
            		LIB_MDL_TITRE..x  = BTN_BACK..x + BTN_BACK..largeur + 6
            		
            	sinon
            		
            		BTN_BACK..Visible = faux
            		LIB_MDL_TITRE..X  = BTN_BACK..x + 6
            		
            	FIN

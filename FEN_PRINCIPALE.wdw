  procedures :
   -
     name : AffichePlan
     procedure_id : 1583373782852769652
     type_code : 14
     code : |1-
      PROCEDURE AffichePlan(nPlan est un entier, bAvecHistorique est un booléen = vrai)
      
      	SI MenuUWPOption > 0 ALORS
      		// Restaure la couleur initiale
      		Zr_MDL_MENU[MenuUWPOption]..CouleurFond = IMG_MDL_FONDMENU..CouleurFond
      		
      		si bAvecHistorique alors
      			Ajoute(Historique, MenuUWPOption)
      		sinon
      			si Historique..Occurrence > 0 alors
      				Supprime(Historique, Historique..Occurrence)
      			fin
      		fin
      	FIN
      	
      	// Change la couleur de sélection
      	MenuUWPOption = nPlan
      	Zr_MDL_MENU[MenuUWPOption]..CouleurFond = 0x6E4715
      	
      	pCallback est une procedure = AffichePlanPrécédent
      	
      	SELON MenuUWPOption
      		// 1 - Clients
      		CAS 1
      			ChangeFenêtreSource(CFI_MDL_SUPPORT, FI_CLIENTS, (Historique..Occurrence), pCallback) 			
      		// 2 - Produits
      		CAS 2
      			ChangeFenêtreSource(CFI_MDL_SUPPORT, FI_PRODUITS, (Historique..Occurrence), pCallback)
      		// 3 - Commandes
      		CAS 3
      			ChangeFenêtreSource(CFI_MDL_SUPPORT, FI_COMMANDES, (Historique..Occurrence), pCallback)
      	FIN
      	
      	si MenuUWPOuvert ALORS
      		ExécuteTraitement(BTN_MENU, trtclic)
      	FIN
     type : 458752
   -
     name : AffichePlanPrécédent
     procedure_id : 1583373782852835188
     type_code : 14
     code : |1-
      PROCEDURE AffichePlanPrécédent()
      
      	AffichePlan(historique[Historique..Occurrence], faux)
